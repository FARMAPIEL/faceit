  "rich-text#s4-comentario-3-desc": {
    "props": {
      "text": "Veo menos acné y un poco menos manchas, textura y tonalidad bien.\nMe gusta el aroma del dermolimpiador, huele natural y no tan químico como otros que he usado, el olor del ácido glicólico no es tan natural pero se siente rico.",
      "blockClass": "s4-comentario-1-desc"
    }
  },
  "rich-text#s4-comentario-3-nombre": {
    "props": {
      "text": "Eloy M.",
      "blockClass": "s4-comentario-1-nombre"
    }
