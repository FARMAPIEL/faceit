  "rich-text#s4-comentario-1-desc": {
    "props": {
      "text": "El Paso 1 me gusta porque sientes limpia la cara, al paso de los días se siente más suave y menos impurezas. Con el Paso 2 se siente super fresca la cara y humectada. Con el Paso 3 he notado la disminución de hinchazón y granitos.",
      "blockClass": "s4-comentario-1-desc"
    }
  },
  "rich-text#s4-comentario-1-nombre": {
    "props": {
      "text": "Antinea E.",
      "blockClass": "s4-comentario-1-nombre"
    }
  },

