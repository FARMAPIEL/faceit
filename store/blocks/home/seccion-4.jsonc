{
  "flex-layout.row#s4": {
    "props": {
      "htmlId":"testimonios",
      "blockClass":"s4"
    },
    "children": [
      "flex-layout.col#s4"
    ]
  },
  "flex-layout.col#s4": {
    "props": {
      "preventVerticalStretch": true,
      "blockClass":"s4"
    },
    "children": [
      "rich-text#s4-titulo",
      "rich-text#s4-subtitulo",
      "flex-layout.row#s4-commentarios"
    ]
  },
  "rich-text#s4-titulo": {
    "props": {
      "text": "Consigue tu ✨Glow Up✨\n**con FACEIT**",
      "textPosition": "CENTER",
      "blockClass": "s4-titulo"
    }
  },
  "rich-text#s4-subtitulo": {
    "props": {
      "text": "Los usuarios que ya lo probaron nos dejaron estas reviews. ¡Ellos sí que saben!",
      "textPosition": "CENTER",
      "blockClass": "s4-subtitulo"
    }
  },

